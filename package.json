    "express-fileupload": "^1.5.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.1.0",
    "express-session": "^1.18.2",
    "google-auth-library": "^10.3.0",
    "googleapis": "^160.0.0",
    "helmet": "^8.1.0",
    "http-errors": "^2.0.0",
    "ioredis": "^5.8.0",
    "jsonwebtoken": "^9.0.2",
    "lightweight-charts": "^5.0.8",
    "morgan": "^1.10.1",
    "nodemailer": "^7.0.6",
    "socket.io": "^4.8.1",
